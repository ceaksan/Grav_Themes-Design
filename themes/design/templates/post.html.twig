{% extends 'partials/base.html.twig' %}

  {% block head_css %}
		{{ parent() }}
  {% endblock %}

  {% block body %}
    {% block body_content %}

    <div class="blog-entries-section">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-lg-7 section-title text-center mb-5">
            <h2 class="heading" data-aos="fade-up" data-aos-delay="100">{{ page.title }}</h2>
            <p class="text-center"><time class="date" datetime="{{ p.date|date("d-m-Y") }}">{{ p.date|date("d-m-Y") }}</time></p>
          </div>
        </div>
        <div class="row">
          <article class="col-12 post-entry">
            {{ page.content|raw }}
          </article>
        </div>
      </div>
    </div>

    {% endblock %}
  {% endblock %}